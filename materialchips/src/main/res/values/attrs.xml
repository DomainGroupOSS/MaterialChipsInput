<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ChipsInput">
        <attr name="placement" format="enum">
            <enum name="vertical" value="0"/>
            <enum name="horizontal" value="1"/>
        </attr>
        <attr name="hint" format="string" />
        <attr name="hintColor" format="color" />
        <attr name="textColor" format="color" />
        <attr name="maxRows" format="integer" />
        <attr name="chipStyle" format="reference" />
        <attr name="chipTextColor" format="color" />
        <attr name="chipTextColorChecked" format="color" />
        <attr name="chipIconVisible" format="boolean" />
        <attr name="chipLetterTileAsIcon" format="boolean" />
        <attr name="chipDefaultIcon" format="reference" />
        <attr name="chipIconTint" format="color" />
        <attr name="chipIconSize" format="dimension" />
        <attr name="chipCloseIconVisible" format="boolean" />
        <attr name="chipCloseIcon" format="reference" />
        <attr name="chipCloseIconTint" format="color" />
        <attr name="chipCloseIconSize" format="dimension" />
        <attr name="chipCheckable" format="boolean" />
        <attr name="chipCheckedIconVisible" format="boolean" />
        <attr name="chipCheckedIcon" format="reference" />
        <attr name="chipBackgroundColor" format="color" />
        <attr name="chipBackgroundColorChecked" format="color" />
        <attr name="chipStrokeWidth" format="dimension" />
        <attr name="chipStrokeColor" format="color" />
        <attr name="showDetailedChip" format="boolean" />
        <attr name="detailedChipTextColor" format="color" />
        <attr name="detailedChipBackgroundColor" format="color" />
        <attr name="detailedChipDeleteIconColor" format="color" />
        <attr name="filterableListBackgroundColor" format="color" />
        <attr name="filterableListTextColor" format="color" />
    </declare-styleable>

    <declare-styleable name="ScrollViewMaxHeight">
        <attr name="maxHeight" format="dimension" />
    </declare-styleable>
</resources>